From d8e3e83fe962343505060fe330e497629237e8bd Mon Sep 17 00:00:00 2001
From: Yuya Hamamachi <yuya.hamamachi.sx@renesas.com>
Date: Mon, 13 Sep 2021 13:41:34 +0900
Subject: [PATCH] Change to use AArch64 flash-writer

Signed-off-by: Yuya Hamamachi <yuya.hamamachi.sx@renesas.com>
---
 starterkit/linux/rom_writer       | 4 ++--
 starterkit/windows/rom_writer.ttl | 4 ++--
 2 files changed, 4 insertions(+), 4 deletions(-)

diff --git a/starterkit/linux/rom_writer b/starterkit/linux/rom_writer
index e8dd491..8bf171b 100755
--- a/starterkit/linux/rom_writer
+++ b/starterkit/linux/rom_writer
@@ -124,7 +124,7 @@ proc file_check {lists} {
 
 proc mot {} {
     global top
-    return $top/AArch32_Flash_writer_SCIF_DUMMY_CERT_E6300400_ULCB.mot
+    return $top/AArch64_Flash_writer_SCIF_DUMMY_CERT_E6300400_ULCB.mot
 }
 
 proc mot_check {} {
@@ -146,7 +146,7 @@ proc cpld_mode {} {
 	"=>" { send "\r" }
     }
     expect {
-	"=>" { send "cpld write 0x00 0x802181fe\r" }
+	"=>" { send "cpld write 0x00 0x8021813e\r" }
     }
     expect {
 	"=>" { send "cpld write 0x80 0x01\r" }
diff --git a/starterkit/windows/rom_writer.ttl b/starterkit/windows/rom_writer.ttl
index 47e9855..e5247a4 100755
--- a/starterkit/windows/rom_writer.ttl
+++ b/starterkit/windows/rom_writer.ttl
@@ -58,7 +58,7 @@ listbox "Auto Type Select" "Which Type ?" auto 1
 ; mot exist => Full Auto
 mot = ""
 if result=1 then
-  sprintf2 mot "%s\AArch32_Flash_writer_SCIF_DUMMY_CERT_E6300400_ULCB.mot" top
+  sprintf2 mot "%s\AArch64_Flash_writer_SCIF_DUMMY_CERT_E6300400_ULCB.mot" top
   filesearch mot
   if result==0 then
     msg = "mot file not exist\nget it from Linux user"
@@ -211,7 +211,7 @@ wait "=>"
 sendln ""
 
 wait "=>"
-sendln "cpld write 0x00 0x802181fe"
+sendln "cpld write 0x00 0x8021813e"
 
 wait "=>"
 sendln "cpld write 0x80 0x01"
-- 
2.31.1

